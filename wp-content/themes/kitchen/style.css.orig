    }
    
/*    subheader*/
    
    .subheader .title h1 {
        font-size: 30px!important;
        line-height: 30px!important;
        padding: 15px 20px!important;
        left: 10%!important;
    }
    
/*   dropdown menu*/
    
    #menu-main-menu > li:hover > ul.sub-menu{
        position: relative!important;
        
        overflow: hidden!important;
         -webkit-transition: max-height 3s ease-out;
        -ms-transition: max-height 3s ease-out;
        -o-transition: max-height 3s ease-out;
        transition: max-height 3s ease-out;
    }
    
    #menu-main-menu > li > ul.hidden-mobile-menu{
/*        max-height: 0px!important;*/
/*        max-height: 700px!important;*/
        -webkit-transition: max-height 3s ease-out;
        -ms-transition: max-height 3s ease-out;
        -o-transition: max-height 3s ease-out;
        transition: max-height 3s ease-out;
    }

    .menu-item > ul.sub-menu {
        overflow: hidden!important;
        position: relative!important;
/*        max-height: 0px!important;        */
    }
    
    .menu-item > ul.sub-menu > li {
        width: 100%;
    }
    
/*    mobile button*/
    
 
    
    #menu-main-menu > li.menu-item-has-children .mobile-dropdown-btn {
        display: block!important;
        z-index: 99;
        cursor: pointer;
        padding: 15px 20px;
        position: absolute;
        top: 0px;
        right: 0px;
        height: 100%;
        font-size: 1.8em;
    }
    
    #menu-main-menu > li.menu-item-has-children:hover {
        background: #2ecc71;
    }
    
    #manu-main-menu > li.mobile-menu  > ul.sub-menu {
        max-height: 0px!important;
        
        -webkit-transition: max-height 1s linear;
        -ms-transition: max-height 1s linear;
        -o-transition: max-height 1s linear;
        transition: max-height 1s linear;
    }
    
    #menu-main-menu > li.mobile-menu-is-open1 {
        background: #2ecc71;
    }
    
    #menu-main-menu > li.mobile-menu-is-close1, #menu-main-menu > li.mobile-menu-is-close1 > a {
        background: #2c3e50!important;
    }
    
    #menu-main-menu > li.mobile-menu-is-open1 > ul.sub-menu {
        max-height: 500px!important;
    }
    
    #menu-main-menu > li.mobile-menu-is-close1 > ul.sub-menu {
        max-height: 0px!important;
        
        -webkit-transition: max-height .4s linear;
        -ms-transition: max-height .4s linear;
        -o-transition: max-height .4s linear;
        transition: max-height .4s linear;
    }
    
}

@media screen and (max-width: 517px){
    /*    subheader*/
    
    .subheader .title h1 {
        font-size: 20px!important;
        line-height: 25px!important;
        padding: 15px 20px!important;
        left: 10%!important;
    }
}




@media screen and (min-width: 768px){
    #navbar {
        display: block;
    }
    
    .menu-item > ul.sub-menu > li {
        width: 170px;
    }
}

/* Display sub menu*/

.menu-item > ul.sub-menu {
    display: block;
    max-height: 0px;
    overflow: hidden;
    position: absolute;
    list-style: none;
    background: #2ecc71;
    padding: 0;
    z-index: 99;
    -webkit-transition: max-height .1s; 
    -moz-transition: max-height .1s; 
    -ms-transition: max-height .1s; 
    -o-transition: max-height .1s; 
    transition: max-height .1s;  
}

/*disable third level of nesting category*/

.menu-item > ul.sub-menu ul.sub-menu {
    display: none;
}

.menu-item:nth-last-child(-n + 2) > ul.sub-menu{
    right: 0;
}

.menu-item > ul.sub-menu a {
    color: #fff;
    text-decoration: none;
}

.menu-item > ul.sub-menu > li {
    padding: 6px 20px 6px 15px;
    font-size: 14px;
    border-bottom: 1px solid #39d67c;
}

.menu-item > ul.sub-menu > li:hover, .menu-item > ul.sub-menu > li:hover a {
    background: #27BC66;
    cursor: pointer;
}

.menu-item:hover > ul.sub-menu {
    position: absolute;
    max-height: 300%;
    display: block;
    overflow: hidden;
}

@-webkit-keyframes smoth-dropdown-menu {
    from { max-height: 0px; }

    to { max-height: 300%}
}

@keyframes smoth-dropdown-menu {
    from { max-height: 0px; }

    to { max-height: 300% }
}

.menu-item:hover > a {
    background: #2ecc71;
}

.menu-item > ul.sub-menu:hover {
    display: block;
    background: #2ecc71;
}

.menu-item > ul.sub-menu > a {
    display: block;
    border-bottom-width: 1px;
    border-bottom-style: solid;
}
.menu-item > ul.sub-menu > a:hover {
    background: #27BC66;
}

#menu-main-menu > li.menu-item-has-children > a .mobile-dropdown-btn {
    display: none;
}
    
