/*
 *  Common styles
 */

body {
    color: #717e8c;
    background: #dfe3e5;
}

/* bootsrap*/

.nav>li>a:focus, .nav>li>a:hover{
     background: none;
}

.row {
    margin-left: 0px;
    margin-right: 0px;
}

.col-lg-1, .col-lg-10, .col-lg-11, .col-lg-12, .col-lg-2, .col-lg-3, .col-lg-4, .col-lg-5, .col-lg-6, .col-lg-7, .col-lg-8, .col-lg-9, .col-md-1, .col-md-10, .col-md-11, .col-md-12, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-sm-1, .col-sm-10, .col-sm-11, .col-sm-12, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-xs-1, .col-xs-10, .col-xs-11, .col-xs-12, .col-xs-2, .col-xs-3, .col-xs-4, .col-xs-5, .col-xs-6, .col-xs-7, .col-xs-8, .col-xs-9{
    padding-left: 0;
    padding-right: 0;
}


/* breadcrumbs */ 

.breadcrumbs .home {
    background: url(images/breadcrumbs_home.png);
    background-repeat: no-repeat;
    background-position: left center;
    padding-left: 30px;
}

.breadcrumbs {
    padding: 10px 15px;
}

.breadcrumbs a, .breadcrumbs .current {
    color: #99AEB0;
    font-size: 12px;
}

/*
 *  Header
 */

.top-bar {
    padding-top: 15px;
    padding-bottom: 15px;
    background: #dfe3e5;
}

#languages-switch {
    position: absolute;
}

#languages-switch select {
      font: 400 12px/1.3 "Helvetica Neue", sans-serif;
      -webkit-appearance: none;
      appearance: none;
      line-height: 1;
      outline: 0;
      color: #2c3e50;
      padding: 0.65em 2.5em 0.55em 0.75em;
      border-radius: 3px;
      background: linear-gradient(#2c3e50, #2c3e50 ) no-repeat,
                  linear-gradient(-135deg, rgba(255,255,255,0) 50%, #bac4c5 50%) no-repeat,
                  linear-gradient(-225deg, rgba(255,255,255,0) 50%, #bac4c5 50%) no-repeat,
                  linear-gradient(#2c3e50 , #2c3e50 ) no-repeat;
      background-color: #bac4c5;
      background-size: 1px 100%, 20px 20px, 20px 20px, 20px 60%;
      background-position: right 20px center, right bottom, right bottom, right bottom;   
}

.top-bar > .phone, .top-bar > .social {
    float: right;
}

.top-bar > .phone {
    padding: 3px 15px 3px 0px;
    font-size: 24px;
    line-height: 24px;
    margin-right: 15px;
    border-right: 1px solid rgba(0, 0, 0, .1);
    vertical-align: middle;
}

.icon-mobile-phone:before {
    content: "\f10b";
}

.col-centered {
    float: none;
    margin: 0 auto;
}

.navbar-header {
    position: absolute;
    padding: 20px 25px;
    z-index: 98;
    background: #fff;
    text-align: center;
}

header .social ul {
    padding: 0px;
}

header .social li a {
    background: #bac4c5;
    color: #2c3e50 !important;
}

header .social li a:hover {
    background: #2ecc71;
    color: #fff !important;
}

.mobnav-button {
    display: none;
}

.mobnav-button {
    position: absolute;
    top: 5px;
    right: 10%;
    z-index: 99;
    cursor: pointer;
}

.mobnav-button > div {
        width: 35px;
        height: 5px;
        background-color: black;
        margin: 6px 0;
}

.navbar {
  height: 50px;  
}

#navbar {
    background: #2c3e50;
}

#navbar > div {
  float: right;  
}

#navbar > div > ul > li > a {
  color: #fff;
  padding: 30px 20px 30px;
  white-space: nowrap;
  text-transform: uppercase;
}

#navbar > div > ul > li > a:hover {
  background: #2ecc71;
}

